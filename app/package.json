{
  "name": "pic-captain",
  "productName": "LookBack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "build": {
    "appId": "com.lookback.app",
    "productName": "LookBack",
    "icon": "resources/icon.png",
    "files": [
      "**/*",
      "!**/.venv/**",
      "!**/.uv/**",
      "!backend/python/__pycache__/**",
      "!**/__pycache__/**"
    ],
    "asarUnpack": [
      "backend/python/**"
    ],
    "mac": {
      "target": {
        "target": "dmg",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "extraResources": [
        {
          "from": "bin/mac",
          "to": "bin/mac",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://mirror.ghproxy.com/https://github.com/anhaohui/RroRef/releases/latest/download/"
      }
    ]
  },
  "scripts": {
    "dev": "concurrently \"tsup electron/main.ts electron/preload.ts --format cjs --out-dir dist-electron --external electron --watch --onSuccess 'electron .'\" \"vite\"",
    "build": "tsc -b && vite build && tsup electron/main.ts electron/preload.ts --format cjs --out-dir dist-electron --external electron && electron-builder",
    "build:mac": "npm run build -- --mac --arm64",
    "build:win": "tsc -b && vite build && tsup electron/main.ts electron/preload.ts --format cjs --out-dir dist-electron --external electron && electron-builder --win",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@plait/common": "^0.92.1",
    "@plait/core": "^0.92.1",
    "body-parser": "^2.2.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1",
    "fs-extra": "^11.3.3",
    "konva": "^10.0.12",
    "lucide-react": "^0.562.0",
    "radash": "^12.1.1",
    "rc-input": "^1.8.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-konva": "^19.2.1",
    "react-masonry-css": "^1.0.16",
    "react-moveable": "^0.56.0",
    "react-selecto": "^1.26.3",
    "tailwind-merge": "^3.4.0",
    "use-image": "^1.1.4",
    "valtio": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
